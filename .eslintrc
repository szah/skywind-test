{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "react/jsx-indent-props": [2, 4],
        "react/jsx-indent": [2, 4],
        "indent": [2, 4, {"SwitchCase": 1}],
        "global-require": 0,
        "react/prefer-stateless-function": 0
    }
}
